# PostgreSQL
POSTGRES_DB=dev_jobs_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme

# Backend — полный URL, готовый к использованию
# ВАЖНО: использовать postgresql:// (sync), НЕ postgresql+asyncpg://
# Alembic использует этот URL напрямую (sync через psycopg2)
# database.py автоматически конвертирует в postgresql+asyncpg:// для async engine
DATABASE_URL=postgresql://postgres:changeme@db:5432/dev_jobs_db

# Secrets
JWT_SECRET_KEY=<generate: python -c "import secrets; print(secrets.token_hex(32))">
INTERNAL_SECRET=<generate: python -c "import secrets; print(secrets.token_urlsafe(32))">

# Demo runtime profile
ENV=dev
EMAIL_ENABLED=false
INTERVIEW_SESSION_BACKEND=memory

# Redis Cache
REDIS_URL=redis://redis:6379/0
CACHE_ENABLED=true
CACHE_TTL_SECONDS=120
